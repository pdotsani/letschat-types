{
  "name": "@letschat/types",
  "version": "1.0.0",
  "description": "Shared TypeScript types library for Let's Chat",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    }
  },
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "build": "tsc && npm run bundle && npm run create-js",
    "create-js": "node scripts/extract-runtime-values.js",
    "bundle": "dts-bundle-generator -o index.d.ts --project tsconfig.json --no-banner src/index.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist index.d.ts index.js index.d.ts.map index.js.map",
    "prepack": "npm run build",
    "prepare": "npm run build",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "typescript",
    "types",
    "letschat"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "typescript": "^5.3.3",
    "dts-bundle-generator": "^9.0.0"
  }
}