{
  "name": "@letschat/types",
  "version": "1.0.0",
  "description": "Shared TypeScript types library for Let's Chat",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
